
# Natural Language Question
# =======================
For the human entry P05067 (Amyloid-beta precursor protein) find the gene start ends in WikiData

# Target Endpoint
# ===============
https://sparql.uniprot.org/sparql/

# Ground Truth Query
# =================
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX uniprotkb: <http://purl.uniprot.org/uniprot/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>


SELECT 
	?protein 
	?begin
	?end
	?chromosome
	?assembly
WHERE {
    {
        BIND(uniprotkb:P05067 AS ?proteinIRI)
        BIND (SUBSTR(STR(?proteinIRI), STRLEN(STR(uniprotkb:))+1) AS ?protein)
    }
    SERVICE <https://query.wikidata.org/sparql> {
        ?wp wdt:P352 ?protein ;
            wdt:P702 ?wg . 
        ?wg p:P644   ?wgss .
        ?wgss ps:P644        ?begin ;
          pq:P1057/wdt:P1813 ?chromosome ;
          pq:P659/rdfs:label ?assembly .
        ?wg p:P645 ?wgse .
        ?wgse ps:P645        ?end ;
          pq:P1057/wdt:P1813 ?chromosome ;
          pq:P659/rdfs:label ?assembly .
        FILTER(lang(?assembly) = "en")
  } 
}

# Predicted Query
# ==============
# https://sparql.uniprot.org/sparql
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX up: <http://purl.uniprot.org/core/>
PREFIX uniprot: <http://purl.uniprot.org/uniprot/>

SELECT ?gene ?geneName ?wikidataID ?geneStart ?geneEnd
WHERE {
  # Find the UniProt entry for Amyloid-beta precursor protein (P05067)
  VALUES (?protein) { (uniprot:P05067) }
  ?protein a up:Protein .
  
  # Get the associated gene
  ?protein up:encodedBy ?gene .
  ?gene rdfs:label ?geneName .

  # Attempt to link to Wikidata using a property like "same as" or equivalent
  OPTIONAL {
    ?protein up:sameAs ?wikidataID . #This might not be the correct property, but it's a common way to link to external resources
  }
  
  #Federate with Wikidata to get gene start and end positions
  OPTIONAL {
    SERVICE <https://query.wikidata.org/sparql> {
      # Assuming Wikidata has information about gene location
      ?wikidataID wdt:P528 ?geneStart .  #Example property for gene start position (This is a placeholder, the correct property needs to be identified)
      ?wikidataID wdt:P529 ?geneEnd .    #Example property for gene end position (This is a placeholder, the correct property needs to be identified)
    }
  }
}

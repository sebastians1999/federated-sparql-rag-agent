
# Natural Language Question
# =======================
Look for enzymes catalyzing reactions with molecules similar to dopamine.

# Target Endpoint
# ===============
https://sparql.uniprot.org/sparql/

# Ground Truth Query
# =================
PREFIX CHEBI: <http://purl.obolibrary.org/obo/CHEBI_>
PREFIX rh: <http://rdf.rhea-db.org/>
PREFIX sachem: <http://bioinfo.uochb.cas.cz/rdf/v1.0/sachem#>
PREFIX up: <http://purl.uniprot.org/core/>

SELECT
  ?protein
  ?rhea
  ?chebi
WHERE {
  # Use the smiles of dopamine CHEBI:18243
  SERVICE <https://idsm.elixir-czech.cz/sparql/endpoint/chebi>{
    ?chebi sachem:similarCompoundSearch [
      sachem:query "NCCc1ccc(O)c(O)c1" ] .
  }
  GRAPH<https://sparql.rhea-db.org/rhea>{
    ?rhea rh:side/rh:contains/rh:compound ?compound .
    ?compound (rh:chebi|(rh:reactivePart/rh:chebi)|(rh:underlyingChebi/rh:chebi)) ?chebi .
  }
  ?protein up:reviewed true ;
    up:annotation ?caa .
  ?caa up:catalyticActivity/up:catalyzedReaction ?rhea .
}

# Predicted Query
# ==============
# https://sparql.rhea-db.org/sparql
PREFIX rhea: <http://rdf.rhea-db.org/>
PREFIX chebi: <http://purl.obolibrary.org/obo/CHEBI_>
PREFIX uniprot: <http://purl.uniprot.org/core/>

SELECT DISTINCT ?enzyme ?enzymeName ?reaction ?reactionEquation
WHERE {
  # Find reactions in Rhea that involve dopamine (or similar molecules)
  {
    ?reaction rhea: Rhea_has_participant chebi:16708 . #CHEBI ID for adenine. I am using this as a placeholder as I don't have the CHEBI ID for dopamine.
  }
  UNION
  {
   ?reaction rhea: Rhea_has_participant ?participant.
   ?participant rdfs:label ?participantLabel.
   FILTER (CONTAINS(LCASE(?participantLabel), "dopamine")) #Looking for dopamine in the participant label
  }

  # Get the reaction equation
  ?reaction rhea:equation ?reactionEquation .

  # Find the enzyme(s) that catalyze the reaction
  ?reaction rhea:enzyme ?enzyme .

  # OPTIONAL: Get enzyme name from UniProt (federated query)
  SERVICE <https://sparql.uniprot.org/sparql> {
    ?enzyme a uniprot:Protein ;
            rdfs:label ?enzymeName .
  }
}
LIMIT 100


# Natural Language Question
# =======================
Find mouse homologs in OMABrowser of human enzymes that catalyze reactions involving sterols (CHEBI:15889). Federating with Rhea and OMABrowser.

# Target Endpoint
# ===============
https://sparql.uniprot.org/sparql/

# Ground Truth Query
# =================
PREFIX CHEBI: <http://purl.obolibrary.org/obo/CHEBI_>
PREFIX lscr: <http://purl.org/lscr#>
PREFIX orth: <http://purl.org/net/orth#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rh: <http://rdf.rhea-db.org/>
PREFIX taxon: <http://purl.uniprot.org/taxonomy/>
PREFIX up: <http://purl.uniprot.org/core/>

SELECT
  DISTINCT
    ?chebi
    ?reaction
    ?humanProtein
    ?mouseProtein
    ?cluster 
WHERE {
  SERVICE <https://sparql.rhea-db.org/sparql> {
    ?reaction rdfs:subClassOf rh:Reaction .
    ?reaction rh:side/rh:contains/rh:compound ?compound .
    ?compound rh:chebi ?chebi .
    ?chebi rdfs:subClassOf* CHEBI:15889
  }

  ?humanProtein up:organism taxon:9606 .
  ?humanProtein up:annotation ?a .
  ?a a up:Catalytic_Activity_Annotation .
  ?a up:catalyticActivity ?ca .
  ?ca up:catalyzedReaction ?reaction .

  SERVICE  <https://sparql.omabrowser.org/sparql> {
    ?cluster a orth:ParalogsCluster .
    ?cluster orth:hasHomologousMember ?node1 , ?node2 .
    ?node1 orth:hasHomologousMember* ?orthoProtein1 .
    ?node2 orth:hasHomologousMember* ?orthoProtein2 .
    ?orthoProtein1 lscr:xrefUniprot ?mouseProtein .
    ?orthoProtein2 lscr:xrefUniprot ?humanProtein .
    # inTaxon mouse
    ?orthoProtein1 orth:organism/<http://purl.obolibrary.org/obo/RO_0002162> taxon:10090 . 
  }
}

# Predicted Query
# ==============
# https://sparql.rhea-db.org/sparql
PREFIX rhea: <http://rdf.rhea-db.org/>
PREFIX chebi: <http://purl.obolibrary.org/obo/CHEBI_>
PREFIX uniprot: <http://purl.uniprot.org/core/>
PREFIX oma: <http://omabrowser.org/ontology/>
PREFIX dc: <http://purl.org/dc/elements/1.1/>

SELECT DISTINCT ?mouse_homolog ?human_enzyme_name ?reaction
WHERE {
  # Find reactions in Rhea involving sterols (CHEBI:15889)
  SERVICE <https://sparql.rhea-db.org/sparql> {
    ?reaction rhea: Rhea_has_reactant chebi:15889 .
    ?reaction dc:description ?reaction_name .
    # Find the enzymes that catalyze these reactions
    ?reaction rhea:rhea_has_ অংশগ্রহণকারী ?participant .
    ?participant rhea:rhea_participant_side rhea:rhea_left . # Assuming enzyme is on the left side.  May need adjustment.
    ?participant rhea:rhea_participant_object ?enzyme_rhea .
  }

  # Find corresponding UniProt entries for these enzymes, assuming they are proteins
  SERVICE <https://sparql.uniprot.org/sparql> {
    ?enzyme_uniprot rdfs:seeAlso ?enzyme_rhea . # Link from Rhea to UniProt
    FILTER CONTAINS(str(?enzyme_uniprot), "uniprot.org") # Ensure it's a UniProt URI
    ?enzyme_uniprot a uniprot:Protein .
    ?enzyme_uniprot uniprot:organism <http://purl.uniprot.org/taxonomy/9606> . # Filter for human proteins
    ?enzyme_uniprot rdfs:label ?human_enzyme_name .
  }

  # Find mouse homologs in OMA Browser
  SERVICE <http://sparql.omabrowser.org/sparql> {
    ?human_enzyme_oma oma:memberOf/oma:genome/oma:organism <http://purl.uniprot.org/taxonomy/9606> . # Human enzyme in OMA
    ?human_enzyme_oma oma:is_homolog_of ?mouse_homolog_oma .
    ?mouse_homolog_oma oma:memberOf/oma:genome/oma:organism <http://purl.uniprot.org/taxonomy/10090> . # Mouse homolog
    ?mouse_homolog_oma rdfs:label ?mouse_homolog .
    FILTER CONTAINS(str(?human_enzyme_oma), "protein") # Ensure it's a protein
  }
}

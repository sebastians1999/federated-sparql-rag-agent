
# Natural Language Question
# =======================
Total number of <reaction> <ec> <protein> links

# Target Endpoint
# ===============
https://sparql.rhea-db.org/sparql

# Ground Truth Query
# =================
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rh:<http://rdf.rhea-db.org/>
PREFIX ec:<http://purl.uniprot.org/enzyme/>
PREFIX up:<http://purl.uniprot.org/core/>

SELECT
  (count(?reaction) as ?reactionEcProteinLinkCount)
WHERE {
  ?reaction rdfs:subClassOf rh:Reaction .
  ?reaction rh:ec ?enzyme
  SERVICE <http://sparql.uniprot.org/sparql> {
    ?protein up:reviewed true .
    ?protein up:enzyme ?enzyme.
  }
}

# Predicted Query
# ==============
# https://sparql.rhea-db.org/sparql
PREFIX rhea: <http://rdf.rhea-db.org/>
PREFIX uniprot: <http://purl.uniprot.org/core/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT COUNT(*) AS ?totalLinks
WHERE {
  # Query Rhea for reactions
  SERVICE <https://sparql.rhea-db.org/sparql> {
    ?reaction a rhea:Reaction .
  }

  # Query UniProt for proteins and EC numbers
  SERVICE <https://sparql.uniprot.org/sparql> {
    ?protein a uniprot:Protein .
    ?protein uniprot:annotation ?ecNumberAnnotation .
    ?ecNumberAnnotation rdf:type uniprot:EC_Annotation .
    ?ecNumberAnnotation uniprot:value ?ecNumber .
  }

  # Assuming a link between Rhea reactions and UniProt EC numbers exists,
  # potentially through a shared EC number. This might require further refinement
  # based on the actual data structure.  This is a simplification.
  FILTER (STR(?reaction) = STR(?ecNumber)) # Attempt to link based on string matching.  This is a simplification and may not be accurate.
}

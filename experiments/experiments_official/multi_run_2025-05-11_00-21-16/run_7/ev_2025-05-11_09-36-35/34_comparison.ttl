
# Natural Language Question
# =======================
Lipids affected by human enzymes

# Target Endpoint
# ===============
https://sparql.swisslipids.org/sparql/

# Ground Truth Query
# =================
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rh: <http://rdf.rhea-db.org/>
PREFIX taxon: <http://purl.uniprot.org/taxonomy/>
PREFIX up: <http://purl.uniprot.org/core/>

SELECT ?swisslipid  {
  ?swisslipid owl:equivalentClass ?chebi .
  SERVICE <https://sparql.rhea-db.org/sparql> {
    ?rhea rh:side/rh:contains/rh:compound ?compound .
	?compound (rh:chebi|(rh:reactivePart/rh:chebi)|(rh:underlyingChebi/rh:chebi)) ?metabolite . 
  }
  SERVICE <https://sparql.uniprot.org/sparql> {
    ?catalyticActivityAnnotation up:catalyticActivity/up:catalyzedReaction ?rhea .
    ?protein up:annotation ?catalyticActivityAnnotation ;
             up:organism taxon:9606 .
  }
}
LIMIT 1

# Predicted Query
# ==============
# https://sparql.uniprot.org/sparql
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX up: <http://purl.uniprot.org/core/>
PREFIX taxon: <http://purl.uniprot.org/taxonomy/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT DISTINCT ?lipidName
WHERE {
  # Find human proteins (enzymes) in UniProt
  SERVICE <https://sparql.uniprot.org/sparql> {
    SELECT DISTINCT ?enzyme ?enzymeName WHERE {
      ?enzyme a up:Protein .
      ?enzyme up:organism taxon:9606 .  # 9606 is the NCBI taxonomy ID for Homo sapiens
      ?enzyme rdfs:label ?enzymeName .
    }
  }

  # Find reactions in Rhea DB that involve these enzymes
  SERVICE <https://sparql.rhea-db.org/sparql> {
    PREFIX rhea: <http://rdf.rhea-db.org/>
    PREFIX chebi: <http://purl.obolibrary.org/obo/CHEBI_>

    ?reaction rhea:equation ?equation .
    ?reaction rhea:directionalReaction ?directionalReaction .
    ?directionalReaction rhea:enzyme ?enzyme . # Link Rhea reaction to enzyme from UniProt

    # Get the ChEBI identifiers of reactants and products in the reaction
    {
      ?directionalReaction rhea:reactant ?chebi .
    }
    UNION
    {
      ?directionalReaction rhea:product ?chebi .
    }
  }

  # Find lipids in SwissLipids that match the ChEBI identifiers
  SERVICE <https://sparql.swisslipids.org/sparql/> {
    PREFIX sl: <http://swisslipids.org/rdf/>
    PREFIX chebi: <http://purl.obolibrary.org/obo/CHEBI_>

    ?lipid sl:chebi ?chebi . # Link SwissLipids lipid to ChEBI ID from Rhea
    ?lipid rdfs:label ?lipidName .
  }
}


# Natural Language Question
# =======================
Retrieve human enzymes that metabolize sphingolipids and are annotated in ChEMBL

# Target Endpoint
# ===============
https://sparql.uniprot.org/sparql/

# Ground Truth Query
# =================
PREFIX CHEBI: <http://purl.obolibrary.org/obo/CHEBI_>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rh: <http://rdf.rhea-db.org/>
PREFIX taxon: <http://purl.uniprot.org/taxonomy/>
PREFIX up: <http://purl.uniprot.org/core/>

SELECT DISTINCT ?protein ?chemblEntry
WHERE {
  SERVICE <https://sparql.rhea-db.org/sparql> {
    ?rhea rdfs:subClassOf rh:Reaction ;
      rh:side/rh:contains/rh:compound/rh:chebi/rdfs:subClassOf+ CHEBI:26739 .
  }
  ?ca up:catalyzedReaction ?rhea .
  ?protein up:annotation/up:catalyticActivity ?ca ;
    up:organism taxon:9606 ;
    rdfs:seeAlso ?chemblEntry .
  ?chemblEntry up:database <http://purl.uniprot.org/database/ChEMBL> .
}
LIMIT 10

# Predicted Query
# ==============
# https://sparql.uniprot.org/sparql
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX up: <http://purl.uniprot.org/core/>
PREFIX taxon: <http://purl.uniprot.org/taxonomy/>

SELECT DISTINCT ?enzyme ?enzymeName
WHERE {
  # Find human enzymes in UniProt
  SERVICE <https://sparql.uniprot.org/sparql> {
    ?enzyme a up:Protein .
    ?enzyme up:organism taxon:9606 .  # Homo sapiens
    ?enzyme rdfs:label ?enzymeName .
    ?enzyme up:annotation ?annot .
    ?annot a up:CatalyticActivity .
  }

  # Find enzymes involved in sphingolipid metabolism in SwissLipids
  SERVICE <https://sparql.swisslipids.org/sparql/> {
    ?reaction <http://rdf.swisslipids.org/vocabulary/enzyme> ?swisslipidsEnzyme .
    ?swisslipidsEnzyme rdfs:label ?enzymeNameSwissLipids .
    FILTER (CONTAINS(?enzymeName, ?enzymeNameSwissLipids))
  }

  # Find enzymes annotated in ChEMBL using Rhea
  SERVICE <https://sparql.rhea-db.org/sparql> {
    ?rheaReaction rdfs:comment ?rheaComment .
    FILTER (CONTAINS(?rheaComment, "ChEMBL"))
    ?rheaReaction <http://purl.obolibrary.org/obo/RO_0002333> ?chebiEntity . # enzyme is a participant in the reaction
    ?chebiEntity rdfs:label ?enzymeNameRhea .
    FILTER (CONTAINS(?enzymeName, ?enzymeNameRhea))
  }
}

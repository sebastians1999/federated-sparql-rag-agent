
# Natural Language Question
# =======================
Find mouse homologs in OMABrowser of human enzymes that catalyze reactions involving sterols (CHEBI:15889). Federating with Rhea and OMABrowser.

# Target Endpoint
# ===============
https://sparql.uniprot.org/sparql/

# Ground Truth Query
# =================
PREFIX CHEBI: <http://purl.obolibrary.org/obo/CHEBI_>
PREFIX lscr: <http://purl.org/lscr#>
PREFIX orth: <http://purl.org/net/orth#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rh: <http://rdf.rhea-db.org/>
PREFIX taxon: <http://purl.uniprot.org/taxonomy/>
PREFIX up: <http://purl.uniprot.org/core/>

SELECT
  DISTINCT
    ?chebi
    ?reaction
    ?humanProtein
    ?mouseProtein
    ?cluster 
WHERE {
  SERVICE <https://sparql.rhea-db.org/sparql> {
    ?reaction rdfs:subClassOf rh:Reaction .
    ?reaction rh:side/rh:contains/rh:compound ?compound .
    ?compound rh:chebi ?chebi .
    ?chebi rdfs:subClassOf* CHEBI:15889
  }

  ?humanProtein up:organism taxon:9606 .
  ?humanProtein up:annotation ?a .
  ?a a up:Catalytic_Activity_Annotation .
  ?a up:catalyticActivity ?ca .
  ?ca up:catalyzedReaction ?reaction .

  SERVICE  <https://sparql.omabrowser.org/sparql> {
    ?cluster a orth:ParalogsCluster .
    ?cluster orth:hasHomologousMember ?node1 , ?node2 .
    ?node1 orth:hasHomologousMember* ?orthoProtein1 .
    ?node2 orth:hasHomologousMember* ?orthoProtein2 .
    ?orthoProtein1 lscr:xrefUniprot ?mouseProtein .
    ?orthoProtein2 lscr:xrefUniprot ?humanProtein .
    # inTaxon mouse
    ?orthoProtein1 orth:organism/<http://purl.obolibrary.org/obo/RO_0002162> taxon:10090 . 
  }
}

# Predicted Query
# ==============
# https://sparql.rhea-db.org/sparql/
PREFIX rhea: <http://rdf.rhea-db.org/>
PREFIX chebi: <http://purl.obolibrary.org/obo/CHEBI_>
PREFIX uniprot: <http://purl.uniprot.org/core/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT DISTINCT ?mouse_homolog
WHERE {
  # Find reactions in Rhea that involve sterols (CHEBI:15889)
  {
    ?reaction rhea: Rhea_has_reactant chebi:15889 .
  }
  UNION
  {
    ?reaction rhea: Rhea_has_product chebi:15889 .
  }

  # Find the enzyme (UniProt ID) that catalyzes the reaction
  ?reaction rhea: Rhea_has_enzyme ?enzyme_uri .
  BIND(IRI(REPLACE(STR(?enzyme_uri), "http://purl.uniprot.org/uniprot/", "http://purl.uniprot.org/core/")) AS ?enzyme)

  # Get the UniProt ID from the enzyme URI
  ?enzyme rdf:type uniprot:Protein .
  BIND(STRAFTER(STR(?enzyme), "http://purl.uniprot.org/core/") AS ?uniprot_id)

  # Find the human protein in UniProt
  SERVICE <https://sparql.uniprot.org/sparql> {
    ?human_protein uniprot:database ?uniprot .
    FILTER(STR(?uniprot) = ?uniprot_id) .
    ?human_protein uniprot:organism <http://purl.uniprot.org/taxonomy/9606> . # Human
    BIND(STR(replace(str(?human_protein), "http://purl.uniprot.org/uniprot/", "", "i")) AS ?human_protein_id)
  }

  # Find the mouse homolog in OMABrowser (assuming a SPARQL endpoint exists)
  SERVICE <http://sparql.omabrowser.org/> { # Replace with the actual OMABrowser SPARQL endpoint
    ?homology_relationship a <http://purl.org/net/orth#OrthologousRelation> .
    ?homology_relationship <http://purl.org/net/orth#source> ?human_gene .
    ?homology_relationship <http://purl.org/net/orth#target> ?mouse_gene .

    FILTER(CONTAINS(STR(?human_gene), ?human_protein_id)) .
    FILTER(CONTAINS(STR(?mouse_gene), "MOUSE")) . # Assuming mouse genes are tagged with "MOUSE"
    BIND(STRAFTER(STR(?mouse_gene), "http://omabrowser.org/gene/") AS ?mouse_homolog)
  }
}

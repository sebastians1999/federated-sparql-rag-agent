
# Natural Language Question
# =======================
Search for Chemical names starting in Japanese, translated to english using allie

# Target Endpoint
# ===============
https://sparql.rhea-db.org/sparql/

# Ground Truth Query
# =================
PREFIX oboInOwl: <http://www.geneontology.org/formats/oboInOwl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT
*
WHERE {
SERVICE <https://data.allie.dbcls.jp/sparql>{
        ?x rdfs:label "1,2,4-トリクロロベンゼン"@ja ;
            rdfs:label ?englishLabel .
        FILTER(lang(?englishLabel) = "en")
    }
  BIND(STR(?englishLabel) AS ?englishLabelStr)
  ?chebi rdfs:label|oboInOwl:hasSynonym ?englishLabelStr .
}

# Predicted Query
# ==============
# https://sparql.uniprot.org/sparql
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX chebi: <http://purl.obolibrary.org/obo/CHEBI_>

SELECT DISTINCT ?chebi_name
WHERE {
  # Query ChEBI for chemical entities with names starting with "Japanese"
  SERVICE <https://sparql.rhea-db.org/sparql> {
    ?chebi a <http://purl.obolibrary.org/obo/CHEBI_24431> . # Chemical entity
    ?chebi rdfs:label ?chebi_name .
    FILTER STRSTARTS(LCASE(?chebi_name), "japanese")
  }
  #OPTIONAL {
  #  SERVICE <some_translation_endpoint> {  # Replace with a real translation endpoint if available
  #    ?chebi_name <translation_property> ?english_name .
  #  }
  #}
}


# Natural Language Question
# =======================
Find mouse homologs in OMABrowser of human enzymes that catalyze reactions involving sterols (CHEBI:15889). Federating with Rhea and OMABrowser.

# Target Endpoint
# ===============
https://sparql.uniprot.org/sparql/

# Ground Truth Query
# =================
PREFIX CHEBI: <http://purl.obolibrary.org/obo/CHEBI_>
PREFIX lscr: <http://purl.org/lscr#>
PREFIX orth: <http://purl.org/net/orth#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rh: <http://rdf.rhea-db.org/>
PREFIX taxon: <http://purl.uniprot.org/taxonomy/>
PREFIX up: <http://purl.uniprot.org/core/>

SELECT
  DISTINCT
    ?chebi
    ?reaction
    ?humanProtein
    ?mouseProtein
    ?cluster 
WHERE {
  SERVICE <https://sparql.rhea-db.org/sparql> {
    ?reaction rdfs:subClassOf rh:Reaction .
    ?reaction rh:side/rh:contains/rh:compound ?compound .
    ?compound rh:chebi ?chebi .
    ?chebi rdfs:subClassOf* CHEBI:15889
  }

  ?humanProtein up:organism taxon:9606 .
  ?humanProtein up:annotation ?a .
  ?a a up:Catalytic_Activity_Annotation .
  ?a up:catalyticActivity ?ca .
  ?ca up:catalyzedReaction ?reaction .

  SERVICE  <https://sparql.omabrowser.org/sparql> {
    ?cluster a orth:ParalogsCluster .
    ?cluster orth:hasHomologousMember ?node1 , ?node2 .
    ?node1 orth:hasHomologousMember* ?orthoProtein1 .
    ?node2 orth:hasHomologousMember* ?orthoProtein2 .
    ?orthoProtein1 lscr:xrefUniprot ?mouseProtein .
    ?orthoProtein2 lscr:xrefUniprot ?humanProtein .
    # inTaxon mouse
    ?orthoProtein1 orth:organism/<http://purl.obolibrary.org/obo/RO_0002162> taxon:10090 . 
  }
}

# Predicted Query
# ==============
# https://sparql.uniprot.org/sparql
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rh: <http://rdf.rhea-db.org/>
PREFIX CHEBI: <http://purl.obolibrary.org/obo/CHEBI_>
PREFIX taxon: <http://purl.uniprot.org/taxonomy/>
PREFIX up: <http://purl.uniprot.org/core/>
PREFIX obo: <http://purl.obolibrary.org/obo/>
PREFIX genex: <http://purl.org/genex#>
PREFIX oma: <http://purl.org/net/orth#>

SELECT DISTINCT ?protein ?mouseGeneSymbol ?humanGeneSymbol
WHERE {
  # Find human enzymes that catalyze reactions involving sterols
  SERVICE <https://sparql.rhea-db.org/sparql> {
    ?reaction rh:side/rh:contains/rh:compound ?compound .
    ?compound rh:chebi ?chebi .
    ?chebi rdfs:subClassOf* CHEBI:15889 .
  }
  ?protein up:annotation/up:catalyticActivity/up:catalyzedReaction ?reaction .
  ?protein up:organism taxon:9606 .
  ?protein rdfs:seeAlso ?ensemblGene . # Link to Ensembl gene

  # Find mouse homologs in OMABrowser
  SERVICE <http://sparql.omabrowser.org/sparql> {
    ?gene_human rdfs:label ?humanGeneSymbol .
    ?gene_human a obo:GENE_0000001 .
    ?gene_human genex:about <http://purl.uniprot.org/taxonomy/9606> .
    ?gene_human oma:hasHomologousMember ?gene_mouse .
    ?gene_mouse genex:about <http://purl.uniprot.org/taxonomy/10090> .
    ?gene_mouse rdfs:label ?mouseGeneSymbol .
  }
  FILTER(STRSTARTS(str(?ensemblGene), "http://identifiers.org/ensembl:")) # Ensure ensemblGene is an Ensembl identifier
  BIND(SUBSTR(STR(?ensemblGene), 32) AS ?ensemblGeneID) # Extract Ensembl ID
  FILTER (CONTAINS(STR(?gene_human), ?ensemblGeneID)) # Filter human genes to match Ensembl ID
}

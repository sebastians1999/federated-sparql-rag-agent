
# Natural Language Question
# =======================
Find all proteins linked to arachidonate (CHEBI:32395)

# Target Endpoint
# ===============
https://sparql.uniprot.org/sparql/

# Ground Truth Query
# =================
PREFIX CHEBI: <http://purl.obolibrary.org/obo/CHEBI_>
PREFIX rh: <http://rdf.rhea-db.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX up: <http://purl.uniprot.org/core/>


SELECT 
    DISTINCT
        ?uniprot
        ?uniprotID
        ?recname
        ?gene
        ?chebi
        ?uniprotName
WHERE {
  SERVICE <https://sparql.rhea-db.org/sparql> {
     VALUES (?chebi) { (CHEBI:32395) }
     ?rhea rh:side/rh:contains/rh:compound ?compound .
     ?compound rh:chebi ?chebi .
     ?chebi up:name ?uniprotName .
  }
  ?uniprot up:annotation/up:catalyticActivity/up:catalyzedReaction ?rhea .
  ?uniprot up:mnemonic ?uniprotID .
  ?uniprot up:recommendedName/up:fullName ?recname .
  OPTIONAL {?uniprot up:encodedBy/skos:prefLabel ?gene .}
}

# Predicted Query
# ==============
# https://sparql.uniprot.org/sparql
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX up: <http://purl.uniprot.org/core/>
PREFIX chebi: <http://purl.obolibrary.org/obo/CHEBI_>

SELECT DISTINCT ?protein ?proteinName
WHERE {
  # Use UniProt as the primary endpoint to find proteins
  SERVICE <https://sparql.uniprot.org/sparql> {
    ?protein a up:Protein .
    ?protein rdfs:label ?proteinName .
    # Assuming a link to ChEBI through a related entity (e.g., involved in a pathway or binding)
    # Replace up:reference with the appropriate predicate if available, for example:
    # up:interaction or similar property indicating association with a chemical entity
    ?protein up:sequence ?sequence . # Added to ensure it's a protein and not another entity
  }
  
  # Use Rhea DB to find reactions involving arachidonate
  SERVICE <https://sparql.rhea-db.org/sparql> {
    # Find reactions that involve arachidonate (CHEBI:32395) as a participant
    ?reaction rdfs:label ?reactionName .
    VALUES ?arachidonate { chebi:32395 }  # Using the provided CHEBI ID
    ?reaction ?participant ?arachidonate . # Assuming ?participant links to reaction participants
    
    # Optionally, filter for reactions that also involve proteins (this part is speculative and may need adjustment)
    ?reaction ?proteinParticipant ?protein . # Link to UniProt protein
  }
}

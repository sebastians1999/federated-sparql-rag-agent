
# Natural Language Question
# =======================
Select all Rhea reactions linked to protein sequences (enzymes and transporters) in UniProtKB

# Target Endpoint
# ===============
https://sparql.rhea-db.org/sparql

# Ground Truth Query
# =================
PREFIX rh: <http://rdf.rhea-db.org/>
PREFIX up: <http://purl.uniprot.org/core/>

# Query 12
# Select all Rhea reactions used to annotate enzyme sequences in UniProtKB
# return the number of UniProtKB entries
# Federated query using a service to UniProt SPARQL endpoint
#
# This query corresponds to the Rhea website query:
# https://www.rhea-db.org/rhea?query=uniprot:*
#
SELECT ?uniprotCount ?rhea ?accession ?equation 
WHERE {
  SERVICE <https://sparql.uniprot.org/sparql> { 
  	SELECT ?rhea (count(?uniprot) as ?uniprotCount) {
      ?uniprot up:annotation/up:catalyticActivity/up:catalyzedReaction ?rhea . 
  	}
  	GROUP BY ?rhea
  }
  ?rhea rh:accession ?accession .
  ?rhea rh:equation ?equation .
}

# Predicted Query
# ==============
# https://sparql.rhea-db.org/sparql
# Prefix declarations for commonly used namespaces
PREFIX rhea: <http://rdf.rhea-db.org/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX uniprot: <http://purl.uniprot.org/core/>

# Select the Rhea reaction identifiers
SELECT DISTINCT ?rheaReaction

WHERE {
  # Subquery to retrieve UniProtKB protein identifiers for enzymes and transporters
  SERVICE <https://sparql.uniprot.org/sparql> {
    SELECT DISTINCT ?protein
    WHERE {
      {
        # Find proteins annotated as enzymes
        ?protein a uniprot:Protein .
        ?protein rdfs:label ?enzymeLabel . #Get the label
        FILTER (CONTAINS(LCASE(?enzymeLabel), "enzyme")) #Filter by the label
      }
      UNION
      {
        # Find proteins annotated as transporters
        ?protein a uniprot:Protein .
        ?protein rdfs:label ?transporterLabel . #Get the label
        FILTER (CONTAINS(LCASE(?transporterLabel), "transporter")) #Filter by the label
      }
    }
  }

  # Use the retrieved UniProtKB protein identifiers to find corresponding Rhea reactions
  SERVICE <https://sparql.rhea-db.org/sparql> {
    ?rheaReaction rhea:equation ?equation . #Get the reaction equation
    ?rheaReaction rdfs:label ?rheaLabel . #Get the label
    ?proteinLink rhea:rheaId ?rheaReaction . #Link the protein to the reaction

    # Filter by the UniProtKB protein identifiers retrieved from the UniProt SPARQL endpoint
    FILTER (?proteinLink IN (SELECT ?protein WHERE {
      SERVICE <https://sparql.uniprot.org/sparql> {
        {
          SELECT DISTINCT ?protein
          WHERE {
                ?protein a uniprot:Protein .
                ?protein rdfs:label ?enzymeLabel . #Get the label
        		FILTER (CONTAINS(LCASE(?enzymeLabel), "enzyme")) #Filter by the label
          }
        }
        UNION
        {
          SELECT DISTINCT ?protein
          WHERE {
                ?protein a uniprot:Protein .
                ?protein rdfs:label ?transporterLabel . #Get the label
        		FILTER (CONTAINS(LCASE(?transporterLabel), "transporter")) #Filter by the label
          }
        }
      }
    }))
  }
}

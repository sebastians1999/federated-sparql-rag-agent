
# Natural Language Question
# =======================
Look for enzymes catalyzing reactions with molecules similar to dopamine.

# Target Endpoint
# ===============
https://sparql.uniprot.org/sparql/

# Ground Truth Query
# =================
PREFIX CHEBI: <http://purl.obolibrary.org/obo/CHEBI_>
PREFIX rh: <http://rdf.rhea-db.org/>
PREFIX sachem: <http://bioinfo.uochb.cas.cz/rdf/v1.0/sachem#>
PREFIX up: <http://purl.uniprot.org/core/>

SELECT
  ?protein
  ?rhea
  ?chebi
WHERE {
  # Use the smiles of dopamine CHEBI:18243
  SERVICE <https://idsm.elixir-czech.cz/sparql/endpoint/chebi>{
    ?chebi sachem:similarCompoundSearch [
      sachem:query "NCCc1ccc(O)c(O)c1" ] .
  }
  GRAPH<https://sparql.rhea-db.org/rhea>{
    ?rhea rh:side/rh:contains/rh:compound ?compound .
    ?compound (rh:chebi|(rh:reactivePart/rh:chebi)|(rh:underlyingChebi/rh:chebi)) ?chebi .
  }
  ?protein up:reviewed true ;
    up:annotation ?caa .
  ?caa up:catalyticActivity/up:catalyzedReaction ?rhea .
}

# Predicted Query
# ==============
# https://sparql.rhea-db.org/sparql
# Find reactions in Rhea involving dopamine and then find enzymes associated with those reactions.
PREFIX rhea: <http://rdf.rhea-db.org/>
PREFIX chebi: <http://purl.obolibrary.org/obo/CHEBI_>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT DISTINCT ?enzyme ?reaction
WHERE {
  # Find reactions in Rhea that involve dopamine as a participant.
  {
    SELECT DISTINCT ?reaction WHERE {
      ?reaction rhea: Rhea_has_participant chebi:15377 .  # Dopamine in ChEBI
    }
  }

  # Find enzymes associated with these reactions using Rhea's UniProt cross-references.
  ?reaction rhea:rheaId ?rheaId .
  SERVICE <https://sparql.uniprot.org/sparql> {
    ?enzyme uniprot:catalystOf rhea:<?rheaId> . # Use rheaId to link to UniProt
    ?enzyme rdf:type uniprot:Protein .
  }
}

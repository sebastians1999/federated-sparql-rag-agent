
# Natural Language Question
# =======================
Lipids affected by human enzymes

# Target Endpoint
# ===============
https://sparql.swisslipids.org/sparql/

# Ground Truth Query
# =================
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rh: <http://rdf.rhea-db.org/>
PREFIX taxon: <http://purl.uniprot.org/taxonomy/>
PREFIX up: <http://purl.uniprot.org/core/>

SELECT ?swisslipid  {
  ?swisslipid owl:equivalentClass ?chebi .
  SERVICE <https://sparql.rhea-db.org/sparql> {
    ?rhea rh:side/rh:contains/rh:compound ?compound .
	?compound (rh:chebi|(rh:reactivePart/rh:chebi)|(rh:underlyingChebi/rh:chebi)) ?metabolite . 
  }
  SERVICE <https://sparql.uniprot.org/sparql> {
    ?catalyticActivityAnnotation up:catalyticActivity/up:catalyzedReaction ?rhea .
    ?protein up:annotation ?catalyticActivityAnnotation ;
             up:organism taxon:9606 .
  }
}
LIMIT 1

# Predicted Query
# ==============
# https://sparql.uniprot.org/sparql
PREFIX up: <http://purl.uniprot.org/core/>
PREFIX taxon: <http://purl.uniprot.org/taxonomy/>
PREFIX rh: <http://rdf.rhea-db.org/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX CHEBI: <http://purl.obolibrary.org/obo/CHEBI_>

SELECT DISTINCT ?protein ?rhea ?chebi_id
WHERE {
  # UniProt: Find human enzymes and the reactions they catalyze
  ?protein up:organism taxon:9606 . # Filter for human proteins (Homo sapiens)
  ?protein up:annotation ?annotation .
  ?annotation a up:Catalytic_Activity_Annotation .
  ?annotation up:catalyticActivity ?catalyticActivity .
  ?catalyticActivity up:catalyzedReaction ?rhea . # ?rhea is the reaction catalyzed by the human enzyme

  # Rhea: Find reactions involving compounds that are lipids or subclasses of lipids
  SERVICE <https://sparql.rhea-db.org/sparql> {
    ?rhea rh:side ?side .
    ?side rh:contains ?participant .
    ?participant rh:compound ?compound .
    ?compound rh:chebi ?chebi_id . # Get the ChEBI identifier for the compound
    ?chebi_id rdfs:subClassOf* CHEBI:24940 . # Filter for compounds that are lipids (CHEBI:24940) or any of their subclasses
  }
}
